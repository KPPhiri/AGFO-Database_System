<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="401.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginPage">
   <children>
         <Button fx:id="NewOwnerRegistrationButton" layoutX="75.0" layoutY="290.0" mnemonicParsing="false" onAction="#pressOwnerRegistration" text="New Owner Registration" />
         <Button fx:id="NewVisitorRegistrationButton" layoutX="463.0" layoutY="290.0" mnemonicParsing="false" onAction="#pressVisitorRegistration" text="New Visitor Registration" />
         <Button fx:id="lgn_btn" layoutX="310.0" layoutY="243.0" mnemonicParsing="false" onAction="#checkLogin" text="Login" />
         <PasswordField fx:id="password_PasswordField" layoutX="252.0" layoutY="187.0" />
         <TextField fx:id="email_TextField" layoutX="253.0" layoutY="145.0" />
         <Label layoutX="201.0" layoutY="150.0" text="Email:" />
         <Label layoutX="178.0" layoutY="192.0" text="Password:" />
         <Label layoutX="201.0" layoutY="57.0" text="ATL Gardens, Farms, and Orchards" />
      <Label fx:id="wrong" layoutX="216.0" layoutY="218.0" text="Incorrect email or password. Please try again." textFill="#dd2c2c" visible="false" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" />
   </children>
</AnchorPane>
