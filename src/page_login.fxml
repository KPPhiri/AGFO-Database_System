<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="496.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginPage">
   <children>
         <Button fx:id="newOwnerRegistrationButton" layoutX="75.0" layoutY="290.0" mnemonicParsing="false" onAction="#pressOwnerRegistration" text="New Owner Registration" />
         <Button fx:id="newVisitorRegistrationButton" layoutX="384.0" layoutY="290.0" mnemonicParsing="false" onAction="#pressVisitorRegistration" text="New Visitor Registration" />
         <Button fx:id="lgn_btn" layoutX="283.0" layoutY="244.0" mnemonicParsing="false" onAction="#checkLogin" text="Login" />
         <PasswordField fx:id="password_PasswordField" layoutX="217.0" layoutY="187.0" />
         <TextField fx:id="username_TextField" layoutX="217.0" layoutY="145.0" />
         <Label layoutX="145.0" layoutY="149.0" text="Username:" />
         <Label layoutX="146.0" layoutY="192.0" text="Password:" />
         <Label layoutX="201.0" layoutY="57.0" text="ATL Gardens, Farms, and Orchards" />
      <Label fx:id="wrong" layoutX="216.0" layoutY="218.0" text="Incorrect email or password. Please try again." textFill="#dd2c2c" visible="false" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" />
      <Label fx:id="login_ErrorMessage" alignment="TOP_CENTER" layoutX="25.0" layoutY="347.0" prefHeight="131.0" prefWidth="607.0" />
   </children>
</AnchorPane>
