<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ConfirmedProperties">
   <children>
      <Label fx:id="confirmedproptag" layoutX="14.0" layoutY="14.0" text="Confirmed Properties">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TableView layoutX="14.0" layoutY="50.0" prefHeight="214.0" prefWidth="570.0">
        <columns>
          <TableColumn fx:id="namecol" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="addrecol" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="citycol" prefWidth="75.0" text="City" />
            <TableColumn fx:id="zipcol" prefWidth="75.0" text="Zip" />
            <TableColumn fx:id="sizecol" prefWidth="75.0" text="Size" />
            <TableColumn fx:id="typecol" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="pubcol" prefWidth="75.0" text="Public" />
            <TableColumn fx:id="commcol" prefWidth="75.0" text="Commercial" />
            <TableColumn fx:id="idcol" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="vericol" prefWidth="75.0" text="Verified By" />
            <TableColumn fx:id="ratcol" prefWidth="75.0" text="Avg. Rating" />
        </columns>
      </TableView>
      <Button fx:id="searchbut" layoutX="14.0" layoutY="339.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="156.0" text="Search Properties" />
      <Button fx:id="managebut" layoutX="221.0" layoutY="313.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="156.0" text="Manage Selected Property" />
      <SplitMenuButton fx:id="searchspin" layoutX="14.0" layoutY="287.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="156.0" text="Search by...">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </SplitMenuButton>
      <TextField fx:id="searchbar" layoutX="14.0" layoutY="313.0" prefHeight="26.0" prefWidth="156.0" promptText="Search Term" />
      <Button fx:id="backbut" layoutX="474.0" layoutY="340.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="111.0" text="Back" />
   </children>
</AnchorPane>
