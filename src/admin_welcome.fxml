<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminWelcome">
   <children>
      <Label fx:id="welcomelabel" layoutX="162.0" layoutY="14.0" text="Welcome">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="visitorsbut" layoutX="42.0" layoutY="56.0" mnemonicParsing="false" onAction="#gotoVisitorList" prefHeight="53.0" prefWidth="155.0" text="View Visitors&#10;       List" />
      <Button fx:id="confirmedbut" layoutX="41.0" layoutY="137.0" mnemonicParsing="false" onAction="#goToConfirmed" prefHeight="54.0" prefWidth="155.0" text="View Confirmed &#10;   Properties" />
      <Button fx:id="approvedbut" layoutX="41.0" layoutY="214.0" mnemonicParsing="false" onAction="#gotoApprovedCrops" prefHeight="54.0" prefWidth="155.0" text="View Approved&#10;Animals and Crop" />
      <Button fx:id="logoutbut" layoutX="141.0" layoutY="300.0" mnemonicParsing="false" onAction="#logOut" prefHeight="46.0" prefWidth="155.0" text="Log Out" />
      <Button fx:id="ownersbut" layoutX="241.0" layoutY="56.0" mnemonicParsing="false" onAction="#gotoOwnerlist" prefHeight="54.0" prefWidth="155.0" text="View Owners&#10;       List" />
      <Button fx:id="unconfirmedbut" layoutX="241.0" layoutY="137.0" mnemonicParsing="false" onAction="#goToUnconfirmed" prefHeight="54.0" prefWidth="155.0" text="View Unconfirmed &#10;      Properties" />
      <Button fx:id="pendingbut" layoutX="241.0" layoutY="214.0" mnemonicParsing="false" onAction="#gotoPendingCrops" prefHeight="54.0" prefWidth="155.0" text="View Pending&#10;Animals and Crop" />
   </children>
</AnchorPane>
